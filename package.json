{
  "name": "table-formatter-for-gitlab",
  "displayName": "Table Formatter for GitLab™",
  "version": "0.0.1",
  "description": "Format the table in the GitLab™ MR and Issue.",
  "author": "ryohidaka <39184410+ryohidaka@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryohidaka/table-formatter-for-gitlab.git"
  },
  "bugs": {
    "url": "https://github.com/ryohidaka/table-formatter-for-gitlab/issues"
  },
  "homepage": "https://github.com/ryohidaka/table-formatter-for-gitlab#readme",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "build-firefox": "plasmo build --target=firefox-mv2 --zip",
    "format": "npx prettier ./src --write"
  },
  "dependencies": {
    "markdown-table-prettify": "^3.6.0",
    "marked": "^12.0.1",
    "plasmo": "0.85.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.2.1",
    "@types/chrome": "0.0.265",
    "@types/node": "20.12.4",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.24",
    "husky": "^9.0.11",
    "prettier": "3.2.5",
    "typescript": "5.4.4"
  },
  "manifest": {
    "permissions": [
      "https://gitlab.com/*"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "table-formatter-for-gitlab@hidaka.dev"
      }
    }
  }
}